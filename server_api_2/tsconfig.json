{
	"compilerOptions": {
		"alwaysStrict": true,
		"esModuleInterop": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"noImplicitAny": true,
		"outDir": "./build",
		"removeComments": true,
		"sourceMap": false,
		"strict": true,
		"target": "ESNext",
		"skipLibCheck": true,
		"noUncheckedIndexedAccess": true
	},
	"include": ["./src", "src/providers/nodemailer/templates/*.html"],
	"exclude": ["node_modules"]
}
